<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- size设置形状的大小 -->
    <size
        android:width="20dp"
        android:height="20dp" />

    <!-- padding设置内间距 -->
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />

    <solid android:color="#68c4ff" />


</shape>